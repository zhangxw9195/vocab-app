#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/D:/node/vocab-app/node_modules/.store/csv-parser@3.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/D:/node/vocab-app/node_modules/.store/csv-parser@3.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/csv-parser@3.2.0/node_modules/csv-parser/bin/csv-parser" "$@"
else
  exec node  "$basedir/../.store/csv-parser@3.2.0/node_modules/csv-parser/bin/csv-parser" "$@"
fi
